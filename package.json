{
  "name": "async-game-server",
  "version": "1.0.0",
  "description": "A bare bones http based server designed for turn based games.",
  "main": "src/app.ts",
  "scripts": {
    "start": "set TS_NODE_FILES=true & node -r ts-node/register/transpile-only src/app.ts",
    "nodemon": "nodemon --watch src/**/*.ts --watch src/**/*.js --exec node -r ts-node/register/transpile-only src/app.ts",
    "compile": "tsc",
    "compiled": "node bin/app.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/mongoose": "^5.11.97",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^6.9.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
